#include <iostream>
#include<math.h>
#include <vector>
using namespace std;
int main() {
    int n, m;
    cin >> n >> m;
    vector<vector<int>> a(n, vector<int>(m, 0));
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cin >> a[i][j];
        }
    }
    int s=0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if(i%2==0){
            	cout<<s[i]<<" ";
			}
        }
        
    }
    return 0;
}
